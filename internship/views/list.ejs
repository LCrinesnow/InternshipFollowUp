<%-include header%>

<div class="weui_panel weui_panel_access">
    <div class="weui_panel_hd">
       <h1>少年，你的实习</h1>
    </div>
    <div class="weui_panel_bd">

        <%interns.forEach(function(intern){%>
        <div>
            <div class="weui_media_box weui_media_text">
                <a href="/detail/<%=intern._id %>">
                    <h4 class="weui_media_title"><%=intern.company %></h4>
                    <p class="weui_media_desc"><%= intern.category %>&nbsp&nbsp<%= intern.createTime %></p>
                </a>
            </div>
        </div>
        <%});%>
    </div>
</div>

<%-include footer%>
