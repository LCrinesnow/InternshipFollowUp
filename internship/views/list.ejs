<%-include header%>

<div class="weui_panel weui_panel_access">
    <div class="weui_panel_hd text-center">
       <h1>少年，你的实习</h1>
    </div>
    <div class="table-responsive">
        <table class="table table-striped table-bordered" >
            <tr>
                <th>公司</th>
                <th>类别</th>
                <th>发布时间</th>
            </tr>
            <%interns.forEach(function(intern){%>
            <tr>
                <td>
                    <a href="/detail/<%=intern._id %>">
                        <%=intern.company %>
                    </a>
                </td>
                <td><%= intern.category %></td>
                <td><%= moment(intern.createTime).locale('zh-cn').format('LL') %></td>

            </tr>
            <%});%>
        </table>
    </div>
    </div>

<%-include footer%>
