<%-include header%>




<div class = "text-center page-title">查看实习</div>
<div class = "container">
    <div class="row">
        <div class="col-xs-8 col-xs-offset-2">
            <div class="article-box">
                <h1 class="text-center">实习消息</h1>
                <p class="text-center">
                    <span class="label label-info"><%=intern.category%></span>
                    <span class="create-time">
                        <span class="text-bold">时间:</span>
                        <%= moment(intern.createTime).locale('zh-cn').format('LL HH:mm:ss')%>
                    </span>
                </p>
                <p><%= intern.content%></p>
            </div>
        </div>
    </div>
</div>

<%-include footer%>
