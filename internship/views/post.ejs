<%-include header%>
    <div class="container">
        <div class="col-xs-4 col-xs-offset-4">
            <form method="post">
                <div class="form-group">
                    <input type="text" class="form-control" name="company" placeholder="公司名称">
                </div>
                 <div class="form-group">
                    <input type="text" class="form-control" name="email" placeholder="内推邮箱或其他联系方式">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" name="category" placeholder="技术or经管（这应该是个选项）">
                </div>
                <div class="form-group">
                    <textarea class="form-control" name="content" placeholder="实习内容"></textarea>
                </div>

                <button type="submit" class="btn btn-default center-block">检查后，确认发布</button>
            </form>
        </div>
    </div>
<%-include footer%>
